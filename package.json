{
  "name": "vikings",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start-ng": "ng serve -o",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "gen-stats": "ng build --prod --stats-json",
    "view-stats": "webpack-bundle-analyzer dist/stats.json",
    "docker-up": "docker-compose up -d --build",
    "docker-up-debug":
      "docker-compose -f docker-compose.debug.yml up -d --build",
    "docker-down": "docker-compose down",
    "dev-proxy":
      "concurrently \"npm run debug\" \"ng serve --proxy-config proxy.conf.json --open\"",
    "start": "node server.js",
    "start_debug": "node --inspect=0.0.0.0:9229 server.js",
    "start_azure_debug":
      "if [[ ${APPSVC_TUNNEL_PORT} != \"\" ]]; then node --inspect=0.0.0.0:$APPSVC_TUNNEL_PORT server.js; else npm run start; fi",
    "local": "WWW=./dist node -r dotenv/config server.js",
    "debug": "WWW=./dist node -r dotenv/config --inspect server.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.0.0",
    "@angular/cdk": "^6.0.0",
    "@angular/common": "6.0.0",
    "@angular/compiler": "6.0.0",
    "@angular/core": "6.0.0",
    "@angular/forms": "6.0.0",
    "@angular/http": "6.0.0",
    "@angular/material": "^6.0.0",
    "@angular/platform-browser": "6.0.0",
    "@angular/platform-browser-dynamic": "6.0.0",
    "@angular/router": "6.0.0",
    "angular-in-memory-web-api": "^0.6.0",
    "body-parser": "^1.18.2",
    "core-js": "^2.5.4",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "hammerjs": "^2.0.8",
    "mongoose": "^5.0.12",
    "rxjs": "^6.1.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "6.0.0",
    "@angular/compiler-cli": "6.0.0",
    "@angular/language-service": "6.0.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^6.0.103",
    "codelyzer": "^4.2.1",
    "concurrently": "^3.5.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "2.7.2",
    "webpack-bundle-analyzer": "^2.11.1",
    "@angular-devkit/build-angular": "~0.6.0"
  }
}
